{
    "name": "react-ui-spinners",
    "version": "1.0.0",
    "description": "CSS React Spinners",
    "main": "index.js",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": " rollup -c",
        "lint": "eslint **/*.js",
        "lint:fix": "prettier-eslint **/*.js --write"
    },
    "husky": {
        "hooks": {
